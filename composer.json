{
	"name": "lykegenes/laravel-countries",
	"description": "Get informations about countries.",
	"keywords": [
		"laravel",
		"bootgrid",
		"datagrid",
		"table",
		"datatable"
	],
	"homepage": "https://github.com/Lykegenes/laravel-countries",
	"license": "MIT",
	"authors": [
		{
			"name": "Patrick Samson",
			"email": "patricksamson236@gmail.com"
		}
	],
	"require": {
		"illuminate/support": "~5.1",
        "mledoze/countries": "~1.8"
	},
	"require-dev": {
		"phpunit/phpunit" : "~5.2",
        "mockery/mockery": "0.9.*",
        "orchestra/testbench": "~3.1",
        "satooshi/php-coveralls": "~1.0",
        "illuminate/foundation": "^5.1.3"
	},
	"autoload": {
		"psr-4": {
			"Lykegenes\\LaravelCountries\\": "src"
		}
	},
    "autoload-dev": {
        "psr-4": {
            "Lykegenes\\LaravelCountries\\Tests\\": "tests"
        }
    },
	"scripts": {
		"test": "phpunit"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0-dev"
		}
	},
    "minimum-stability": "dev",
    "prefer-stable": true
}
